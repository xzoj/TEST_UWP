<Page
    x:Class="BaiTapCuoiMon.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:BaiTapCuoiMon"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <CommandBar HorizontalAlignment="Left" DefaultLabelPosition="Bottom">
            <AppBarButton Icon="Add" Label="Add" Click="Add"/>
            <AppBarButton Icon="View" Label="View" Click="View"/>
        </CommandBar>
        <Frame x:Name="ContentFrame" Width="1000" Height="1000" Margin="100, 100,100,0"/>
    </Grid>
</Page>
